#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int c=0,d=0;
    for(int i=0;i<n/2;i++)
    {
        c=c+a[i];
    }
    for(int i=n/2;i<n;i++)
    {
        d=d+a[i];
    }
    cout<<c<<endl<<d;
}
