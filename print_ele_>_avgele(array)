#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,c=0;
    cin>>n;
    int a[n],s=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    for(int i=0;i<n;i++)
    {
        s=s+a[i];
    }
    s=s/n;
    for(int i=0;i<n;i++)
    {
    if(a[i]>=s)
    c++;
    }
    cout<<c;
}
