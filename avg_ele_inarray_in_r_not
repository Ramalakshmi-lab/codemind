#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    cin>>n;
    int a[n],c=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int s=0;
    for(int i=0;i<n;i++)
    {
        s=s+a[i];
    }
    s=s/n;
    for(int i=0;i<n;i++)
    {
    if(s==a[i])
    {
        c=c+1;
        break;
    }
    }
    if(c==1)
    cout<<"True";
    else
    cout<<"False";
}
