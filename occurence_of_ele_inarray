#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,c=0;
    cin>>n;
    int s;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>s;
    for(int i=0;i<n;i++)
    {
        if(s==a[i])
        {
            c=c+1;
        }
    }
   if(c<1)
   cout<<0;
   else
   cout<<c;
}
